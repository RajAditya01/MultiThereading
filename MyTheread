class MyThread extends Thread {
    public void run(){   }
        //task
        for(int i=10;i>=1;i--){   
        System.out.println("Thread is->"+i);
        try{
            Thread.sleep(2000);
        }
        catch(Exception e){}
        }

}
public static void main(String[] args) {
    MyThread t1 = new Thread();
    t1.start()
}